{
  "name": "rally-clientmetrics",
  "version": "6.0.2",
  "description": "Metrics aggregation for Rally Software",
  "main": "dist/rallymetrics.cjs.js",
  "jsnext:main": "dist/rallymetrics.es.js",
  "module": "dist/rallymetrics.es.js",
  "scripts": {
    "lint": "eslint src",
    "docs": "grunt jsdoc",
    "test": "jest src",
    "build": "rollup --config --environment FORMAT:es && rollup --config --environment FORMAT:cjs && rollup --config --environment FORMAT:umd && rollup --config --environment MINIFY,FORMAT:umd",
    "lint:staged": "lint-staged",
    "prettier": "prettier --write --print-width=100 --single-quote=true --trailing-comma=es5 --parser=flow 'src/**/*.js' 'rollup.config.js'",
    "webpack:analyze": "webpack --debug --progress --config webpack.dev.config.js --json > webpack-stats.json",
    "prepublish": "npm run build && npm run test && npm run docs"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --print-width 100 --single-quote true --trailing-comma es5 --parser flow",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "https://github.com/RallySoftware/clientmetrics.git"
  },
  "author": "Rally Software",
  "contributors": [
    {
      "name": "Matt Parrish",
      "email": "matt.r.parrish@gmail.com"
    },
    {
      "name": "Matt Greer",
      "email": "matt.e.greer@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RallySoftware/clientmetrics/issues"
  },
  "files": [
    "dist",
    "doc",
    "src"
  ],
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.3.2",
    "babel-preset-stage-2": "6.22.0",
    "eslint": "3.19.0",
    "eslint-config-rally": "4.0.0",
    "eslint-plugin-jasmine": "2.2.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-rally": "1.2.5",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-whitespace": "^0.1.3",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-jsdoc": "2.1.0",
    "grunt-release": "0.14.0",
    "jest": "19.0.2",
    "lint-staged": "3.4.0",
    "pre-commit": "1.2.2",
    "prettier": "1.2.2",
    "rollup": "0.41.6",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-json": "2.1.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "1.0.2",
    "sinon": "2.1.0",
    "webpack": "2.3.2"
  },
  "dependencies": {
    "uuid": "3.0.1"
  },
  "publishConfig": {
    "registry": "http://npm-publish.f4tech.com"
  }
}
